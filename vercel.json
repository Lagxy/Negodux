{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|build)/(.*)$",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)$",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "database",
        "CACHE_DRIVER": "array",
        "VIEW_COMPILED_PATH": "/tmp/views"
    }
}